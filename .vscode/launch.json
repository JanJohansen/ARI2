{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/src/server/main.ts",
            "stopOnEntry": false,
            "args": [],
            "cwd": "${workspaceRoot}/dist/server/",
            "preLaunchTask": null,            
            "runtimeExecutable": null,
            "runtimeArgs": [ "--nolazy" ],
            "env": { "NODE_ENV": "development" },
            "console": "externalTerminal",
            "sourceMaps": true,
            "outDir": "${workspaceRoot}/dist/server"
        },
        {
            "name": "Attach",
            "type": "node",
            "request": "attach",
            "address": "localhost",
            "port": 9229,
            "protocol": "inspector",
            "sourceMaps": true
        }
    ]
}